<?xml version="1.0" encoding="UTF-8"?>

<ehcache> 

<defaultCache eternal="true" maxElementsInMemory="100" overflowToDisk="false" />

<cache name="Details"
maxElementsInMemory="500"
eternal="true"
overflowToDisk="false"> 

</cache>

<cache name="StationLocation"
maxElementsInMemory="500"
eternal="true"
overflowToDisk="false"> 

</cache>

<cache
name="org.hibernate.cache.StandardQueryCache"
maxElementsInMemory="500"
eternal="false"
timeToLiveSeconds="120" >
</cache>

<cache name="query.UserCredentials"
maxElementsInMemory="500"
eternal="false"
timeToLiveSeconds="86400" >

</cache>

<cache name="query.InsertDetails"
maxElementsInMemory="500"
eternal="false"
timeToLiveSeconds="86400" >

</cache>

<cache name="query.FetchDetails"
maxElementsInMemory="500"
eternal="false"
timeToLiveSeconds="86400" >

</cache>

</ehcache>

